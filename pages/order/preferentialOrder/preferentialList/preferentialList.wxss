.main-tabs{
    background-color: #fff;
}


.preferential-list{
    margin-top: 10px;
    border-top: 1px solid #ddd;
    border-bottom: 1px  solid  #ddd;
    background-color: #fff;
}
.preferential-list .item{
     padding: 10px;
}
.preferential-list  navigator{
    border-bottom: 1px  solid  #f4f4f4;
}
.preferential-list  navigator:last-child{
    border-bottom: 0;
}

.preferential-list .item view text{
    float: right;
    font-size: 16px;
}
.preferential-list .item .time{
    margin-top: 10px;
    font-size: 12px;
    color: #a3a3a3;
    
}
.preferential-list .item .time text{
    color: #60a031;
    font-size: 12px;
}