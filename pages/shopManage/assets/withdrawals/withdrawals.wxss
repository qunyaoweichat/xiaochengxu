 .withdrawals .bank{
     position: relative;
    background-color: #fff;
    padding: 15px;
    margin-bottom: 15px;
    margin-top: 15px;
}
.withdrawals .bank navigator{
      display: inline-block;
}
.withdrawals .bank .pic{
    float: left;
    width: 60px;
    height: 60px;
    border-radius: 60px;
    padding: 2px;
}
.withdrawals .bank .pic image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.withdrawals .bank .iconfont{
    position: absolute;
    top: 0;
    right: 15px;
    line-height: 90px;
    width: 20px;
}
.withdrawals .bank .inner{
    padding-left: 80px;
    padding-right: 20px;
}
.withdrawals .bank .inner .name{
    margin-top: 8px;
}
.withdrawals .bank .inner .title{
    margin-top: 10px;
}
.withdrawals .bank .inner .title text{
    background-color: #fe504f;
    border-radius: 5px;
    margin-right: 10px;
    color: #fff;
    padding: 0 8px;
}

.withdrawals .form{
    margin-top: 15px; 
}
.withdrawals .form .row{
    padding: 15px 12px;
    background-color: #fff;
}
.withdrawals .form .row label{
    float: left;
    width: 70px;
    line-height: 30px;
    text-align: right;
}
.withdrawals .form .row .inpt{
    padding-left: 80px;
}
.withdrawals .form .row .inpt input{
    width: 100%;
    height: 30px;
    line-height: 30px;
    outline: none;
    border: none;
}
.withdrawals .title{
    font-size: 12px;
    color: #666666;
    padding: 5px 5px 5px 15px;
}
.withdrawals .form .row-tele .yz{
    text-align: left;
}
.withdrawals .form .row-tele .inpt{
    width: 42%;
}
.withdrawals .row-tele navigator{
    float: right;
    text-align: center;
    padding: 0 5px;
    line-height: 2;
    color: #888;
    border: solid 1px #65ae51;
    border-radius: 5px;
}