<!-- 提现 -->
<view class='withdrawals'>
    <view class='bank'>
        <navigator hover-class='none' url=''>
            <view class='pic'>
                <image src="/images/ka4.jpg"></image>
            </view> 
            <text class='iconfont icon-right'></text>
            <view class='inner clearfix'>
                <view class='name'>中国工商银行</view>
                <view class='title'><text>个人</text>何建</view>
            </view>
        </navigator>
    </view>

    <view class="form-record">
        <view class='list'>
            <view class="row">
                <view class="c">店铺名称</view>
                <view class="c ">黑石旗舰店</view>
            </view>
            <view class="row">
                <view class="c">可用余额</view>
                <view class="c font-add">10</view>
            </view>
        </view>
    </view>

    <view class='form'>
        <view class='row'>
            <label>金额（元）</label>
            <view class='inpt'>
                <input placeholder="最多可转出34.32元"></input>
            </view>
        </view>
    </view>

    <view class='title'>大额充值建议分批充值，单笔建议不超过5万元</view>
   
    <view class='form'>
        <view class='row row-tele'>
            <label class='yz'>验 证 码</label>
            <navigator hover-class='none' url=''>发送验证码</navigator>
            <view class='inpt'>
                <input placeholder="输入短信验证码"></input>
            </view>
        </view>
    </view>

    <view class='public-btn'>
        <button type="primary" formType="submit">提交申请</button>
    </view>

    <view class='record'>
        <navigator hover-class='none' url='../withdrawalsRecord/withdrawalsRecord'>提现记录</navigator>
    </view>
</view>