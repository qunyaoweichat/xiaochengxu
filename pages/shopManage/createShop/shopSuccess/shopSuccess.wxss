.shop-success .title{
    padding: 15px 12px;
    text-align: center;
    margin-top: 15px;
    background-color: #fff;
    border-top: solid 1px #e5e5e5;
    border-bottom: solid 1px #e5e5e5;
}
.shop-success .title view{
    font-size: 1em;
}
.shop-success .title .nav{
    font-size: 1.3em;
    font-weight: bold;
    margin-top: 5px;
    margin-bottom: 15px;
    color: #E51C23;
}
.shop-success .text_title{
    padding: 12px;
    line-height: 20px;
    font-size: 12px;
}
.shop-success .connect{
    padding: 12px;
    font-size: 12px;
    color: #E51C23;
}
.shop-success .connect navigator{
    display: inline-block;
}
.shop-success .code{
    padding: 15px 12px;
    text-align: center;
    margin-top: 15px;
    background-color: #fff;
    border-top: solid 1px #e5e5e5;
    border-bottom: solid 1px #e5e5e5;
}
.shop-success .code  view{
    margin-bottom: 10px;
}
.shop-success .code .pic image{
    width: 50%;
}
.shop-success .btn{
    margin: 20px 0;
}
.shop-success .btn navigator{
    line-height: 34px;
    font-size: 14px;
    padding: 0 20px;
    display: inline-block;
    background-color: #65ae51;
    border: 1px solid #2baf2b;
    color: #fff;
    border-radius: 5px;
}