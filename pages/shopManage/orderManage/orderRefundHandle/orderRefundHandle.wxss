.order-refundhandle .refundhandl-status{
    padding: 30px;
    text-align: center;
    font-size: 20px;
}
.order-refundhandle .refundhandl-tips{
    margin-top: 0;
}
.order-refundhandle .refundhandl-tips text{
    color: #666;
}
.order-refundhandle .shop-infor .phone{
    float: right;
}
.order-refundhandle .refundhandle-list .list{
    line-height: 44px;
    background: #fff;
    padding: 0 15px;
    border-bottom: 1px solid #ddd;
}
.order-refundhandle .refundhandle-list .list .price{
    color: #f00;
}
.order-refundhandle .refundhandle-list .list text{
    float: right;
}
.order-refundhandle .refundhandle-record{
    background: #fff;
    line-height: 60px;
    text-align: center;
    color: #60a031;
    border-bottom: 1px solid #ddd;
}



.order-refundhandle  .submit{
    margin: 15px;
    text-align: center;
    display: flex;
}
.order-refundhandle .submit .btn{
    flex: 1;
    line-height: 34px;
    font-size: 14px;
    display: inline-block;
    background-color: #65ae51;
    border: 1px solid #2baf2b;
    color: #fff;
    border-radius: 5px;
    text-align: center;
}
.order-refundhandle .submit .btn-white{
    margin-right: 10px;
    background: #fff;
    border: solid 1px #ccc;
    color: #000;
}
