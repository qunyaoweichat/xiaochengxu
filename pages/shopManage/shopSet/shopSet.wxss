/* 店铺设置*/
.shop-set .title{
    padding-top: 15px;
    padding-left: 15px;
}
.shop-set .form-list .item{
    padding:12px;
}
.shop-set .form-list .item .text{
    line-height: 23px;
}
.shop-set .form-list .item .inputs{
    text-align: right;
}
.shop-set .form-list .item .shop-pic{
    line-height: 50px;
}

.shop-set .form-list .item .pic{
    text-align: right;
}
.shop-set .form-list .item .pic image{
    width: 50px;
    height: 50px;
}

/* 页脚 */
.tabBar .tabBar-item .iconfont{
    font-size: 22px;
    text-align: center;
}
.tabBar .on{
    color: #3eb135;
}