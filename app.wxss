page{
    color: #333;
    min-height: 100%;
    background: #f5f5f5;
    font-size: 14px;
}
checkbox{
    transform: scale(0.5)
}

.tabBar{
    position: fixed;
    width: 100%;
    bottom: 0;
    z-index: 10;
    display: flex;
    background: #fff;
    border-top: 1px solid #ddd;
}
.tabBar .tabBar-item{
    position: relative;
    flex: 1;
    text-align: center;
    color: #888;
    margin: 5px 0;
    z-index: 10;
}
.tabBar .tabBar-item .iconfont{
    font-size: 20px;
    
}
.tabBar .tabBar-item .text{
    font-size: 14px;
}
.tabBar .tabBar-item .drop-list{
    position: absolute;
    width: 100%;
    bottom: 100%;
    background: #fff;
    border: 1px solid #ddd;
    border-bottom: none;
    box-sizing: border-box;
    margin-bottom: 5px;
}
.tabBar .tabBar-item .drop-list .item{
    line-height: 40px;
    margin: 0 7px;
    border-bottom: 1px solid #ddd;
}
.tabBar .tabBar-item .drop-list .item:last-child{
    border: none;
}
.tabBar .tabBar-item.collector .iconfont{
    color: #eeb924;
}
.tabBar~view{
    padding-bottom: 50px;
}

/*  ----------商品列表--------  */

/*默认大图列表  */
.goods-list{
    display: flex;
    flex-wrap: wrap;
    padding: 10px;
}
.goods-list .item{
    width: 100%;
    margin-bottom: 10px;
}
.goods-list .item-inner{
    position: relative;
    background: #fff;
}
.goods-list image{
    width: 100%;
    height: 300px;
    display: block;
}
.goods-list .desc{
    padding: 10px;
}
.goods-list .title{
    line-height: 22px;
}
.goods-list .content{
    color: #1aad19;
}
.goods-list .desc .content{
    line-height: 30px;
}
.goods-list .desc .content .iconfont{
    float: right;
    font-size: 20px;
}

/*小图列表  */
.goods-list.small-list{
    padding: 5px;
}
.goods-list.small-list .item{
    width: 50%;
    margin-bottom: 0;
}
.goods-list.small-list .item-inner{
    margin: 5px;
}
.goods-list.small-list image{
    height: 170px;
}
.goods-list.small-list .desc{
    padding: 10px 10px 5px;
}
.goods-list.small-list .title{
    height: 44px;
    overflow: hidden;
}

/* 一大两小  */
.goods-list.big-small{
    padding: 5px;
}
.goods-list.big-small .item{
    width: 50%;
}
.goods-list.big-small .item-inner{
    margin: 5px;
}
.goods-list.big-small image{
    height: 170px;
}
.goods-list.big-small .title{
    height: 44px;
    overflow: hidden;
}
.goods-list.big-small .item:nth-child(3n-2){
    width: 100%;
    margin: 0;
}
.goods-list.big-small .item:nth-child(3n-2) image{
    height: 300px;
}

/*列表形式   */
.goods-list.inline-list .item-inner{
    padding: 10px;
}
.goods-list.inline-list .pic{
    float: left;
}
.goods-list.inline-list .pic image{
    width: 100px;
    height: 100px;
}
.goods-list.inline-list .desc{
    margin-left: 110px;
    padding: 0;
    position: relative;
    height: 100px;
}
.goods-list.inline-list .content{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
}

/* 极简模式 */
.goods-list.simple-style{
    padding: 10px 0;
}
.goods-list.simple-style .desc{
    position: absolute;
    bottom: 0;
    right: 0;
    width: 100%;
    background: rgba(0, 0, 0, .8);
    color: #fff;
    padding: 5px 10px;
    box-sizing: border-box;
}
.goods-list.simple-style .desc .title{
    float: left;
    width: 60%;
    height: 30px;
    line-height: 30px;
    overflow: hidden;
    text-overflow: ellipsis;
}
.goods-list.simple-style .desc .content{
    text-align: right;
    color: #fff;
}
.goods-list.simple-style .desc .content .iconfont{
    display: none
}

/* 小图的极简模式 */
.goods-list.simple-style.small-list .desc{
    width: auto;
    padding: 0 10px;
}
.goods-list.simple-style.small-list .title{
    display: none;
}

/*大小图的极简模式  */
.goods-list.simple-style.big-small .item:nth-child(3n-1) .desc,.goods-list.simple-style.big-small .item:nth-child(3n) .desc{
    width: auto;
    padding: 0 10px;
}
.goods-list.simple-style.big-small .item:nth-child(3n-1) .title,.goods-list.simple-style.big-small .item:nth-child(3n) .title{
    display: none;
}
.goods-list.simple-style.inline-list .desc{
    position: relative;
    background: none;
    color: #333;
    width: auto;
    
}

/* 列表的极简模式  */
.goods-list.simple-style.inline-list .desc{
    padding: 10px 0 0 0;
}
.goods-list.simple-style.inline-list .desc .content{
    color: #1aad19;
    text-align: left;
}
.goods-list.simple-style.inline-list .desc .title{
    width: 100%;
    line-height: 22px;
    height: 44px;
}
.goods-list.simple-style.inline-list .desc .content .iconfont{
    display: block;
}

/*   无标题  */
.goods-list.notitle .title{
    display: none !important;
}

/*  无价格  */
.goods-list.noprice .price{
    display: none !important;
}
.goods-list.noprice .iconfont{
    float: none !important;
}

/*  无购物车  */
.goods-list.nocar .iconfont{
    display: none !important;
}


/*其他N种组合，由于样式细节调整，导致这块的不可描述，自己看class命名理解吧！！！！  */
.goods-list.notitle .desc{
    padding: 5px 10px !important;
}
.goods-list.noprice.notitle .desc,.goods-list.noprice.notitle.nocar .desc{
    padding: 0 !important;
}
.goods-list.noprice.notitle .iconfont{
    position: absolute;
    right: 10px;
    bottom: 5px;
}

.goods-list.notitle.nocar .desc{
    padding: 5px 10px !important
}
.goods-list.small-list.notitle.nocar .desc{
    padding: 0px 10px !important
}
.goods-list.inline-list.notitle .title{
    display: block !important;
}
.goods-list.inline-list.noprice .price{
    display: block !important;
}
.goods-list.inline-list.noprice .iconfont{
    float: right !important;
}


/* tabs切换的头 */
.main-tabs{
    position: fixed;
    z-index: 9;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    border-top: 1px solid #ddd;
    border-bottom: 1px  solid  #ddd;
    background: #fff;
    
}
.main-tabs>view{
    flex: 1;
    text-align: center;
    line-height: 44px; 
}
.main-tabs .on{
    color: #1aad19;
    border-bottom: 2px solid #1aad19;
}
.main-tabs+.list-wrap{
    padding-top: 54px;
}
/*  ----------商品列表--------  */
/* 公用表单，例如修改密码 */
.form-list{
    margin: 15px 0;
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd
}
.form-list .item{
    position: relative;
    display: block;
    background: #fff;
    border-bottom: 1px solid #ddd;
    padding: 8px 10px;
}
.form-list .item:last-child{
    border: none;
}
.form-list .item .text{
    float: left;
    width: 70px;
    line-height: 26px;
}
.form-list .item .inputs{
    margin-left: 70px;
}
.form-list picker{
    line-height: 26px;
}

/* 公用的列表  左内容，中内容，右箭头*/
/* 例如 ：我的设置首页 */
.public-list{
    border-top: 1px solid #f4f4f4;
    background-color: #fff;
    overflow: hidden;
    margin-top: 10px;
}
 .public-list .item{
    background-color: #fff;
    display: block;
    line-height: 45px;
    padding: 0px 12px;
    border-bottom: 1px solid #f4f4f4;
    overflow: hidden;
}
.public-list .item .lf{
    float: left;
    color: #888;
    font-size: 15px;
    font-family: Helvetica, Microsoft YaHei, Arial;
    height: 45px;
    width: 90px;
    overflow: hidden;
}
.public-list .item .middle{
    padding-left: 90px;
    padding-right: 20px;
    line-height: 45px;
} 
.public-list .item .iconfont{
    float: right;
    line-height: 45px;
    text-align:right;
    font-size: 18px;
    color: #BABABA;
    width: 18px;
}


/* 订单列表(包含购物车的)  */
.shop-item{
    margin: 14px 0;
    border-top: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9;
}
.order-box{
    background: #fff;
    padding: 10px 14px;
    margin-bottom: 14px;
}
.order-heard{
    position: relative;
    height: 50px;
    line-height: 50px;
    padding: 0 10px;
    border-bottom: 1px solid #d9d9d9;
    background-color: #f9fcfc;
}
.order-heard image{
    height: 30px;
    width: 30px;
    border-radius: 50%;
    vertical-align: middle;
    margin-right: 10px;
}
.order-heard .name{
    vertical-align: middle;
}
.order-heard .status{
    float: right;
    color: #1aad19;
}
.order-list{
    padding-left: 16px;
    background-color: #fff;
}
.order-list .item{
    padding: 14px 14px 14px 0;
    border-bottom: 1px solid #f4f4f4;
}
.order-list .item .pic{
    float: left;
    margin-right: 14px;
}
.order-list .item .pic image{ 
    height: 80px;
    width: 80px; 
}
.order-list .item .detail{
    line-height: 20px;
}
.order-list .item .name{
    height: 40px;
    font-size: 13px;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.order-list .item .sku{
    height: 20px;
    margin: 4px 0;
    font-size: 12px;
    color: #666;
}
.order-list .item .infor{
    width: 100%;
    font-size: 15px;
    color: #666;
}

.order-list .item .infor .price{
    color: #1aad19
}
.order-list .item .infor .num{
    float: right
}

/*  下单页面下面的fixed框  */
.bottom-infor{
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
    width: 100%;
    z-index: 99
    
}
.bottom-infor .btn{
    float: right;
    width: 35%;
    background: #1aad19;
    height: 46px;
    line-height: 46px;
    text-align: center;
    color: #fff;
    font-size: 16px;
}
.bottom-infor .price{
    line-height: 44px;
    margin-left: 14px;
    font-weight: bold;
    color: #1aad19;
}

/* radio 单选  如修改个人资料页*/
.public-radio .wx-radio-input{
    width:15px;
    height:15px;
}
.public-radio{
    margin-right: 10px;
}

/* 错误提示 如修改个人资料页*/
